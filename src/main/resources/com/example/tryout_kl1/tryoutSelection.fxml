<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.tryout_kl1.Controller.TryoutSelectionController">
<padding>
    <Insets bottom="20" left="20" right="20" top="20" />
</padding>


    <center>
        <TableView fx:id="tryoutTable" prefHeight="400.0" prefWidth="480.0">
            <columns>
                <TableColumn fx:id="idColumn" prefWidth="100" text="ID" />
                <TableColumn fx:id="nameColumn" prefWidth="300" text="Nama Tryout" />
                <TableColumn fx:id="actionColumn" prefWidth="70" text="Aksi" />
            </columns>
        </TableView>


    </center>
    <top>
    <Button onAction="#goBackToDashboard" text="Kembali Ke Dashboard" />
    </top>

</BorderPane>
